[numthreads(10,1,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    float3 vertex = vertices[id.x];
    vertex = vertex + float3(1.0,0.0,0.0);
    if (true)
    {
        
    }

    vertices[id.x] = vertex;
}
